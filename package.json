{
    "name": "caritas-online-beratung-frontend",
    "title": "Caritas Online-Beratung",
    "version": "1.4.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/CaritasDeutschland/caritas-onlineBeratung-frontend.git"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=development RELEASE_VERSION=$npm_package_version npm run gulp -- serve",
        "build": "cross-env NODE_ENV=production RELEASE_VERSION=$npm_package_version npm run gulp -- build && gulp add-versioning",
        "gulp": "node --max-old-space-size=8192 ./node_modules/.bin/gulp",
        "release": "standard-version",
        "mk": "node masterkey.js",
        "version": "echo $npm_package_version"
    },
    "dependencies": {
